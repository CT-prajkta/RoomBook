<h1 align="center">Signup for Room Book</h1>

<%= form_for @user , url: user_path(@user)do |f| %>
    <p>
        <%= f.label :username %> <br/>
        <%= f.text_field :username %> 
    </p>
    <p>
        <%= f.label :password %> <br/>
        <%= f.text_field :password %> 
    </p>
    <p>
        <%= f.submit %> 
    </p>
<% end %>
